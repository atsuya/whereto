{
  "name": "whereto",
  "version": "0.1.0",
  "description": "A Chrome Extension that lists domains where the page you visited sent a request to.",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "https://*/*",
    "http://*/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "whereto",
    "default_popup": "popup.html",
    "default_icon": {
      "32": "icon32.png"
    }
  }
}
